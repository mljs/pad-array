  "description": "Function to fill an array in Javascript",
  "main": "src/index.js",
  "directories": {
    "lib": "src",
    "test": "test"
  },
  "scripts": {
    "eslint": "eslint src test",
    "eslint-fix": "npm run eslint -- --fix",
